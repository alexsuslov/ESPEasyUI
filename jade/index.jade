- var lodash = 'https://cdnjs.cloudflare.com/ajax/libs/lodash.js/4.5.0/lodash.min.js';
- var backbone = 'https://cdnjs.cloudflare.com/ajax/libs/backbone.js/1.2.3/backbone-min.js';

html
  head
    meta(charset="utf-8")
    title= title
    link(rel='stylesheet', href=bootstrap)
    script(src=jquery)
    script(src=lodash)
    script(src=backbone)

  body
    include lib/nav
    .container
      .login.block
      .main.block
      .config.block
      .hardware.block
      .devices.block
      .device.block
      .tools.block
      .footer
        hr
        h6.text-center= footer
    include templates
    include lib/app



